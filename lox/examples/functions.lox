fun sayHi(first, last) {
    print "Hi, " + first + " " + last + "!";
}

sayHi("Dear", "Reader");

fun fibonacci(n) {
  if (n <= 1) return n;
  return fibonacci(n - 2) + fibonacci(n - 1);
}

var start = clock();

for (var i = 0; i < 20; i = i + 1) {
  print fibonacci(i);
}

var end = clock();
print end - start;
